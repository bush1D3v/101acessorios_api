{
  "name": "101acessorios_api",
  "version": "1.0.0",
  "description": "API RESTful do projeto 101 Acessórios",
  "main": "./src/main.ts",
  "author": "Victor José Lopes Navarro",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "bun",
    "typescript",
    "jest",
    "docker",
    "eslint",
    "mit",
    "prisma",
    "redis",
    "typescript",
    "postgresql",
    "yup"
  ],
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker-build": "docker build -t 101acessorios_api .",
    "docker-start": "docker run --name 101acessorios_api -p 3001:3001 101acessorios_api || docker start 101acessorios_api",
    "docker-stop": "docker stop 101acessorios_api",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "jest": "jest --detectOpenHandles",
    "jest:watch": "jest --watch",
    "jest:cov": "jest --coverage",
    "jest:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "jest:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.0",
    "@prisma/client": "5.9.1",
    "@types/cors": "^2.8.17",
    "@types/ioredis": "^5.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "nestjs-zod": "^3.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "prisma": "^5.9.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
